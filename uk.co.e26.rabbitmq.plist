<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>uk.co.e26.rabbitmq</string>

  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/usr/bin:/usr/local/bin</string>
    <key>HOME</key>
    <string>/Users/edd</string>
  </dict>

  <key>ProgramArguments</key>
  <array>
    <string>rabbitmq-server</string>
    <string>-detached</string>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/var/log/launchd.log</string>

  <key>StandardErrorPath</key>
  <string>/var/log/launchd_error.log</string>
  
  <key>WorkingDirectory</key>
  <string>/tmp</string>

</dict>
</plist>